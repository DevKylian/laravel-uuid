    "type": "plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Kylian Dev",
            "email": "contact@kyliandev.fr"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php" : "^7.4|^8.0",
